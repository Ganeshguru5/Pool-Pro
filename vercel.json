{
  "version": 3,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "@next_public_api_url",
    "OTHER_ENV_VAR": "@other_env_var"
  },
  "github": {
    "silent": true
  }
}
